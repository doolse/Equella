{
  "private": true,
  "config": {
    "buildlang": "target/resources/lang",
    "devlang": "../target/scala-2.12/classes/lang",
    "devjs": "../target/scala-2.12/classes/web/reactjs",
    "psglob": "output/*/*.js",
    "dist": "target/resources/web/reactjs"
  },
  "scripts": {
    "install": "cross-env-shell \"psc-package install && mkdirp ${npm_package_config_devlang} ${npm_package_config_buildlang}\"",
    "clean": "rm -rf target/ output/ node_modules/ bower_components/ && yarn install",
    "dev": "cross-env-shell parcel watch --no-autoinstall --public-url=./reactjs/ --out-dir=${npm_package_config_devjs} entrydev/*.html",
    "dev:build": "yarn run bundleps && cross-env-shell parcel build --public-url=./reactjs/ --out-dir=${npm_package_config_devjs} entrybuild/*.html entrybuild/uploadlist.js",
    "dev:uploadlist": "cross-env-shell parcel build --out-dir=${npm_package_config_devjs} entrybuild/uploadlist.js",
    "build": "yarn install && npm-run-all build:*",
    "build:bundles": "yarn run bundleps && cross-env-shell parcel build --public-url=./reactjs/ --out-dir=${npm_package_config_dist} entrybuild/*.html entrybuild/uploadlist.js",
    "build:langbundle": "cross-env-shell parcel build entrybuild/genlang.js --no-minify --no-source-maps --target node --out-dir=target && cross-env-shell \"node target/genlang.js > ${npm_package_config_buildlang}/jsbundle.json\"",
    "compileps": "pulp build",
    "bundleps": "npm-run-all compileps && npm-run-all --parallel bundleps:*",
    "bundleps:main": "cross-env-shell \"purs bundle \"${npm_package_config_psglob}\" -m OEQ.MainUI.Main --main OEQ.MainUI.Main -o entrybuild/main.js\"",
    "bundleps:selection": "cross-env-shell \"purs bundle \"${npm_package_config_psglob}\" -m OEQ.SelectionUI.Main --main OEQ.SelectionUI.Main -o entrybuild/selection.js\"",
    "bundleps:uploadlist": "cross-env-shell \"purs bundle \"${npm_package_config_psglob}\" -m Uploads.UploadList --main Uploads.UploadList -o entrybuild/uploadlist.js\"",
    "bundleps:langbundle": "cross-env-shell \"purs bundle \"${npm_package_config_psglob}\" -m Tools.GenLangStrings --main Tools.GenLangStrings -o entrybuild/genlang.js\""
  },
  "dependencies": {
    "@date-io/luxon": "^1.1.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@types/luxon": "^1.2.2",
    "@types/react-autosuggest": "^9.3.5",
    "@types/react-color": "^2.13.6",
    "@types/react-redux": "^5.0.7",
    "@types/react-swipeable-views": "^0.12.1",
    "@types/redux-logger": "^3.0.5",
    "@types/sprintf-js": "^1.1.0",
    "@types/uuid": "^3.4.4",
    "axios": "^0.18.0",
    "create-react-class": "^15.6.2",
    "downshift": "^2.2.2",
    "es6-object-assign": "^1.1.0",
    "history": "^4.7.2",
    "jspolyfill-array.prototype.find": "^0.1.3",
    "parcel-bundler": "^1.11.0",
    "luxon": "^1.11.3",
    "material-ui-pickers": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-beautiful-dnd": "^7.1.2",
    "react-color": "^2.14.1",
    "react-dom": "^16.8.3",
    "react-redux": "^5.0.7",
    "react-swipeable-views": "^0.12.13",
    "react-visibility-sensor": "^3.11.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "sprintf-js": "^1.1.1",
    "timeago-react": "^2.0.0",
    "typescript-fsa": "^2.5.0",
    "typescript-fsa-reducers": "^0.4.5",
    "uuid": "^3.2.1"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.0.7",
    "argparse": "^1.0.10",
    "combined-stream2": "^1.1.2",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.5",
    "css-vars-ponyfill": "^1.4.0",
    "envify": "^4.1.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "psc-package": "^3.0.1",
    "pulp": "^12.3.0",
    "purescript": "0.12.0",
    "typescript": "^3.0.3"
  }
}
